action (BuildMeowAudioInfo) {
  type (Search)
  description (Makes a meow audio info, aka a playlist, to play.)
  collect {
    input (meowAudio) {
      type (MeowAudio)
      min (Optional) max (Many)
      default-init {
        intent {
          goal: FindMeow
        }
      }
    }
  }
  output (audioPlayer.AudioInfo)
}
